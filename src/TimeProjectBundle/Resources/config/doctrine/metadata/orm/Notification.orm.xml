<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Notification" table="notification">
    <indexes>
      <index name="idSociete_idx" columns="idAdmin"/>
    </indexes>
    <id name="idnotification" type="integer" column="idNotification">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="intituleTitre" type="string" column="intitule_titre" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contenu" type="string" column="contenu" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isvalidation" type="integer" column="isValidation" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="iddate" type="string" column="idDate" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idmission" type="string" column="idMission" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idcategorieMission" type="string" column="idCategorie_mission" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idemploye" type="string" column="idEmploye" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idsociete" type="integer" column="idSociete" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idadmin" target-entity="Admin" fetch="LAZY">
      <join-columns>
        <join-column name="idAdmin" referenced-column-name="idAdmin"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
