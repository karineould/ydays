<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Societe" table="societe">
    <indexes>
      <index name="idSecteur_idx" columns="idSecteur_societe"/>
      <index name="idAdmin_idx" columns="idAdmin"/>
    </indexes>
    <id name="idsociete" type="integer" column="idSociete">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="societecol" type="string" column="Societecol" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idsecteurSociete" target-entity="SecteurSociete" fetch="LAZY">
      <join-columns>
        <join-column name="idSecteur_societe" referenced-column-name="idSecteur_societe"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idadmin" target-entity="Admin" fetch="LAZY">
      <join-columns>
        <join-column name="idAdmin" referenced-column-name="idAdmin"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
