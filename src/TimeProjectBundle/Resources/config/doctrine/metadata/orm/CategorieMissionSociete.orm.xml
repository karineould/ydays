<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CategorieMissionSociete" table="categorie_mission_societe">
    <indexes>
      <index name="idCategorie_mission_idx" columns="idCategorie_mission"/>
      <index name="idSociete_mission_idx" columns="idSociete"/>
    </indexes>
    <id name="idcategorieMissionSociete" type="integer" column="idCategorie_mission_societe">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idsociete" target-entity="Societe" fetch="LAZY">
      <join-columns>
        <join-column name="idSociete" referenced-column-name="idSociete"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idcategorieMission" target-entity="CategorieMission" fetch="LAZY">
      <join-columns>
        <join-column name="idCategorie_mission" referenced-column-name="idCategorie_mission"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
