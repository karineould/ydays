<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CategorieMission" table="categorie_mission">
    <id name="idcategorieMission" type="integer" column="idCategorie_mission">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="label" type="string" column="label" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="couleur" type="string" column="couleur" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
